<mvc:View controllerName="DemoTable2DemoTable2.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Table id="Randomid" inset="true">
						<columns>
							<Column minScreenWidth="Medium" demandPopin="true" hAlign="Left">
								<Text text="Date"/></Column>
							<Column minScreenWidth="Medium" demandPopin="true" hAlign="Left">
								<Text text="Doc Type"/></Column>
							<Column width="10em">
								<Text text="Document Description"/></Column>
							<Column minScreenWidth="Medium" demandPopin="true" hAlign="Right">
								<Text text="Document No"/></Column>
							<Column minScreenWidth="Medium" demandPopin="true" hAlign="Right">
								<Text text="Debit Amount"/></Column>
							<Column minScreenWidth="Medium" demandPopin="true" hAlign="Right">
								<Text text="Credit Amount"/></Column>
							<Column hAlign="Right">
								<Text text="Balance"/></Column>
							<Column minScreenWidth="Medium" hAlign="Right">
								<Text text="Text"/></Column>
						</columns>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>